{
    "version": 1,
    "project": "PyBOP",
    "project_url": "https://github.com/pybop-team/pybop",
    "repo": ".",
    "build_command": [
        "python -m pip install build",
        "python -m build",
        "python -m pip wheel -w {build_cache_dir} {build_dir}"
    ],
    "default_benchmark_timeout": 180,
    "build_cache_dir": ".asv/build_cache",
    "build_dir": ".asv/build",
    "branches": ["develop"],
    "environment_type": "virtualenv",
    "matrix": {
        "req":{
           "pybamm": [],
           "numpy": [],
           "scipy": [],
           "pandas": [],
           "pints": []
       }
    },
    "env_dir": ".asv/env",
    "results_dir": ".asv/results",
    "html_dir": ".asv/html"
}
